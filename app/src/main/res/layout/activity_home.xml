<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
	 xmlns:app="http://schemas.android.com/apk/res-auto"
	 xmlns:tools="http://schemas.android.com/tools"
	 android:layout_width="match_parent"
	 android:layout_height="match_parent"
	 tools:context=".SheikhActivity.HomeActivity">



	 <androidx.recyclerview.widget.RecyclerView
		  android:id="@+id/recyclerView_Sheikh"
		  android:layout_width="@dimen/layout_width_zero"
		  android:layout_height="@dimen/layout_height_zero"
		  app:layout_constraintBottom_toBottomOf="parent"
		  app:layout_constraintEnd_toEndOf="parent"
		  app:layout_constraintStart_toStartOf="parent"
		  app:layout_constraintTop_toTopOf="parent" />

	 <Button
		  android:id="@+id/btn_PlayAll_SheikhActivity"
		  android:layout_width="wrap_content"
		  android:layout_height="wrap_content"
		  android:layout_marginBottom="@dimen/layout_margin_8dp"
		  android:background="@color/colorPrimary"
		  android:textColor="@color/white"
		  android:elevation="@dimen/elevation_2dp"
		  android:text="@string/play_all_quran"
		  android:drawableEnd="@drawable/ic_play"
		  android:padding="@dimen/padding_10dp"
		  android:drawablePadding="@dimen/padding_10dp"
		  app:layout_constraintBottom_toBottomOf="parent"
		  app:layout_constraintEnd_toEndOf="parent"
		  app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>